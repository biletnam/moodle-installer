{
    "name": "covex-nn/moodle-installer",
    "description" : "Composer installer for moodle packages",
    "type": "composer-plugin",
    "license": "MIT",
    "require" : {
        "php" : ">=5.3.3",
        "composer-plugin-api": "~1.0",
        "symfony/filesystem": "~2.3"
    },
    "require-dev" : {
        "composer/composer": "1.0.*@dev",
        "covex-nn/joos" : "~4.0",
        "covex-nn/phpqatools" : "~1.0"
    },
    "authors": [
        {
            "name": "Andrey F. Mindubaev",
            "email": "andrey.mindubaev@apnet.ru",
            "homepage": "http://github.com/covex-nn"
        }
    ],
    "autoload": {
        "psr-0": { "JooS\\Composer\\": "src/" }
    },
    "extra": {
        "class": "JooS\\Composer\\Plugin",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "bin-dir" : "bin",
        "preferred-install" : "dist"
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
