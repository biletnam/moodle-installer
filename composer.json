  },
  "repositories": [
    {
      "type" : "git", 
      "url" : "https://github.com/composer/composer"
    }
  ], 
  "authors": [
    {
      "name": "Andrey F. Mindubaev",
      "email": "andrey.mindubaev@apnet.ru",
      "homepage": "http://github.com/covex-nn"
    }
