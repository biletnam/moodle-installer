{
  "name": "covex-nn/moodle-installer",
  "description" : "Composer installer for moodle packages", 
  "type": "composer-installer",
  "license": "MIT",
  "require" : {
    "php" : ">=5.3.3"
  }, 
  "require-dev" : {
    "composer/composer": "1.0.*@dev", 
    "covex-nn/joos" : "~4.0", 
    "theseer/directoryscanner" : "~1.1", 
    "covex-nn/phpqatools" : "~1.0"
  },
  "repositories": [
    { "type" : "composer", "url" : "https://raw.github.com/covex-nn/phpqatools/master" }
  ], 
  "authors": [
    {
      "name": "Andrey F. Mindubaev",
      "email": "andrey.mindubaev@apnet.ru",
      "homepage": "http://github.com/covex-nn"
    }
  ],
  "autoload": {
    "psr-0": { "JooS\\Composer\\": "src/" }
  },
  "extra": {
    "class": "JooS\\Composer\\MoodleInstaller", 
    "branch-alias": {
      "dev-master": "1.3.x-dev"
    }
  }, 
  "config": {
    "bin-dir" : "bin", 
    "preferred-install" : "dist"
  }, 
  "minimum-stability" : "dev", 
  "prefer-stable" : true
}
